<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".GReculcerView">

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/rv_entrenadores"
        android:layout_width="330dp"
        android:layout_height="262dp"
        android:layout_marginBottom="272dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent" />

    <TextView
        android:id="@+id/tv_total_likes"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="100dp"
        android:text="0"
        app:layout_constraintBottom_toTopOf="@+id/rv_entrenadores"
        app:layout_constraintEnd_toEndOf="@+id/rv_entrenadores"
        app:layout_constraintStart_toStartOf="@+id/rv_entrenadores" />
</androidx.constraintlayout.widget.ConstraintLayout>